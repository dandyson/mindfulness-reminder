{
  "manifest_version": 3,
  "name": "Mindfulness Reminder",
  "version": "1.1",
  "description": "A mindfulness reminder extension",
  "permissions": ["storage", "alarms", "notifications"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/mindful_original.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "images/mindful_original.png",
    "48": "images/mindful_original.png",
    "128": "images/mindful_original.png"
  }
}